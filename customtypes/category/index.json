  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Slug",
          "placeholder": "Category URL slug"
        }
      },
      "name": {
        "type": "Text",
        "config": {
          "label": "Name",
          "placeholder": "Category name"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Image",
          "constraint": {
            "width": 1200,
            "height": 630
          },
          "thumbnails": [
            {
              "name": "thumbnail",
              "width": 300,
              "height": 300
            }
          ]
        }
      },
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta Title",
          "placeholder": "SEO meta title"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "SEO meta description"
        }
      }
    }
  }
}